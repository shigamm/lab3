{{#each objects}}
<div class="col-12 col-sm-8 col-md-4 col-lg-4 col-xl-4 col-xxl-4 g-3">
  <div class="card" data-bs-toggle="modal" data-bs-target="#modal" id="object-{{id}}">
    <img src="{{img}}" class="card-img-top img-fluid" alt="-">
    <div class="card-body">
      <h5 class="card-title">{{name}}</h5>
      <p class="card-text">{{description}}</p>
    </div>
    
  </div>
</div>
{{/each}}